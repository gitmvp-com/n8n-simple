{
  "name": "n8n-simple",
  "version": "0.1.0",
  "description": "Minimal viable version of n8n - workflow automation platform",
  "main": "dist/backend/index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --exec ts-node src/backend/index.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "npm run build && node dist/backend/index.js",
    "start:frontend": "cd frontend && npm run preview",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "1.12.0",
    "bcryptjs": "2.4.3",
    "express": "5.1.0",
    "helmet": "8.1.0",
    "ioredis": "5.3.2",
    "lodash": "4.17.21",
    "sqlite3": "5.1.7",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "20.17.50",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  }
}
